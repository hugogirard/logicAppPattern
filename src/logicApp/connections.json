{
  "managedApiConnections": {
    "azuredatafactory": {
      "api": {
        "id": "@appsetting('api-id')"
      },
      "connection": {
        "id": "@appsetting('connection-id)"
      },
      "connectionRuntimeUrl": "@appsetting('connectionRuntimeUrl)",
      "authentication": {
        "type": "Raw",
        "scheme": "Key",
        "parameter": "@appsetting('azuredatafactory-connectionKey')"
      }
    }
  }
}
